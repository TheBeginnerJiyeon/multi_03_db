SELECT * FROM emp;

SELECT empno, ename, job FROM emp;

SELECT DISTINCT empno FROM emp;

SELECT ALL job, deptno FROM emp;

SELECT deptno, empno, ename, sal, sal*12+comm AS money, comm FROM emp  WHERE ename>'S'  ORDER BY money DESC;